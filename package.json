{
  "name": "calvin-communities",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node bin/www",
    "build": "ng build",
    "test": "yarn && npm run testcases && node_modules/.bin/eslint server/**/*.js && cd webapp/ && ng lint ",
    "lint": "./node_modules/.bin/eslint *.js server/*.js server/api/v1/*.js server/api/v1/modules/**/*.js ",
    "testcases": "npm run truncatedb && ./node_modules/.bin/mocha *.js server/api/v1/modules/**/*.spec.js",
    "coverage": "npm run truncatedb && istanbul cover node_modules/.bin/_mocha -- -u exports -R spec server/api/v1/modules/**/*.spec.js",
    "e2e": "ng e2e",
    "serve": "concurrently \"ng build --watch\" \"nodemon bin/www\"",
    "installdb": "node ./server/dbcreation.js",
    "truncatedb": "node ./server/dbcleanup.js",
    "services": "concurrently \"node server/counters.service\" && \"node server/tools.service\" && \"node server/member.service\" ",
    "svc.counter": "node server/counters.service",
    "svc.tool": "node server/tools.service",
    "svc.member": "node server/member.service",
    "svc.invite": "node server/invite.service"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.6",
    "@angular/cdk": "github:angular/cdk-builds",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "github:angular/material2-builds",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "async": "^2.4.1",
    "body-parser": "^1.17.2",
    "cassandra-driver": "^3.2.2",
    "chai": "^4.0.2",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "hammerjs": "^2.0.8",
    "kafka-node": "^1.6.2",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "material-design-icons": "^3.0.1",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "supertest": "^3.0.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
